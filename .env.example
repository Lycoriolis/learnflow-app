# Firebase Configuration (replace with your own values)
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
VITE_FIREBASE_APP_ID=your-app-id 
VITE_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Auth0 Configuration (if using Auth0)
PUBLIC_AUTH0_DOMAIN=your-auth0-domain
PUBLIC_AUTH0_CLIENT_ID=your-auth0-client-id

# Security Settings
VITE_ADMIN_EMAILS=admin@example.com,another-admin@example.com
VITE_CSRF_SECRET=your-long-random-string-here

# Development Settings
VITE_USE_FIREBASE_EMULATOR=false
